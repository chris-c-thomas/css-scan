{
    "name": "css-scanner-cli",
    "version": "1.0.0",
    "description": "A CLI tool to scan websites for unused CSS using Playwright and Ink.",
    "main": "dist/cli.js",
    "type": "module",
    "bin": {
        "css-scan": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node dist/cli.js",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "cli",
        "ink",
        "react",
        "playwright",
        "css"
    ],
    "author": "Christopher Carl Thomas",
    "license": "MIT",
    "dependencies": {
        "ink": "^5.0.0",
        "ink-spinner": "^5.0.0",
        "ink-text-input": "^6.0.0",
        "meow": "^13.0.0",
        "playwright": "^1.40.0",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/react": "^18.2.0",
        "ts-node": "^10.9.0",
        "typescript": "^5.3.0",
        "xo": "^0.56.0"
    }
}